
public class HVArticle {
	
	private String id;
	private String cat;
	private String day;
	private String time;
	
	public HVArticle(String id, String category, String day, String time){
		this.id = id;
		this.cat = category;
		this.day = day;
		this.time = time;
	}
	
	public String id(){
		return this.id;
	}
	public String cat(){
		return this.cat;
	}
	public String day(){
		return this.day;
	}
	public String time(){
		return this.time;
	}

}
